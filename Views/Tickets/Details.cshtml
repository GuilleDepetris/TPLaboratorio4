@model TrabajoFinal_Laboratorio4.Models.Ticket
<link href="~/css/detailsAndDelete.css" rel="stylesheet" />
@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Ticket</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            <p>Afiliado</p>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.afiliado.apellido)
        </dd>
        <dt class = "col-sm-2">
            <p>Fecha de Solicitud</p>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.fechaSolicitud)
        </dd>
        <dt class = "col-sm-2">
            <p>Nombre Ticket</p>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.observacion)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Editar</a> |
    <a asp-action="Index">Volver a la lista</a>
</div>
